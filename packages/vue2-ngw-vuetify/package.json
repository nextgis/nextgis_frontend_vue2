{
  "name": "@nextgis/vue2-ngw-vuetify",
  "version": "1.0.0",
  "description": "Vuetify components for NextGIS Web",
  "main": "index.js",
  "module": "lib/vue2-ngw-vuetify.esm-bundler.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@nextgis/leaflet-map-adapter": "^2.0.0-alpha.0",
    "@nextgis/ngw-map": "^2.0.0-alpha.0",
    "@nextgis/utils": "^2.0.0-alpha.0",
    "vue": "2.x",
    "vue-class-component": "7.x",
    "vue-property-decorator": "9.x",
    "vuetify": "^2.x",
    "vuex-module-decorators": "1.x"
  },
  "devDependencies": {
    "@nextgis/build-tools": "^2.0.0-alpha.0",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vuetify": "^2.5.10"
  },
  "buildOptions": {
    "name": "VuetifyNgwComponents",
    "tsbuild": true,
    "externals": [
      "vuetify/lib"
    ],
    "formats": [
      "esm-bundler"
    ]
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "dev": "node ../build-tools/lib/build.js",
    "prod": "npm run dev -- --release",
    "lint": "eslint ./src/**/*.ts --fix --c ../../.eslintrc",
    "watch": "npm run dev -- --watch"
  },
  "keywords": [
    "NextGIS",
    "MAP"
  ],
  "author": "NextGIS",
  "files": [
    "index.js",
    "lib"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nextgis/nextgis_frontend_vue2/tree/master/packages/vue2-ngw-vuetify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextgis/nextgis_frontend_vue2.git"
  },
  "bugs": {
    "url": "https://github.com/nextgis/nextgis_frontend_vue2/issues"
  }
}
